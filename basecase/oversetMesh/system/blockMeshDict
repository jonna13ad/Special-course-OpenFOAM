boundary //define patches, walls, empty etc.
(
    // right hand rule, thumb points into domain
	
	inlet
	{
		type patch;
		faces
		(
			(5 4 7 6)
			(4 5 21 20)
			(5 6 22 21)
			(6 7 23 22)
			(7 4 20 23)
		);
	}

	extruderWall
    {
        type wall;
        faces
        (
            (16 20 21 17) //wall inside
			(17 21 22 18)
			(18 22 23 19)
			(19 23 20 16)			
			(32 33 37 36) //wall outside
			(33 34 38 37)
			(34 35 39 38)
			(35 32 36 39)
			(16 17 33 32) //bottom side
			(17 18 34 33)
			(18 19 35 34)
			(19 16 32 35)
			(36 37 38 39) //top face
        );
    } 

	overset
	{
		type overset; 
		faces
		(
			(0 1 2 3)	//core
			(0 8 9 1)	//inner (3-7 mm)
			(1 9 10 2)
			(2 10 11 3)
			(3 11 8 0)
			(8 11 47 44)	//middle (7-11 mm)
			(9 8 44 45)
			(10 9 45 46)
			(11 10 46 47)
			(44 47 27 24) //outer (11-25 mm)
			(45 44 24 25)
			(46 45 25 26)
			(47 46 26 27)
			(24 27 31 28)	// z 0-12 mm
			(25 24 28 29)
			(26 25 29 30)
			(27 26 30 31)
			(28 31 43 40)	// z 12-40 mm
			(29 28 40 41)
			(30 29 41 42)
			(31 30 42 43)
			(51 50 49 48) //top cover center
			(48 49 53 52) //top cover top edges
			(49 50 54 53)
			(50 51 55 54)
			(51 48 52 55)
			(40 43 55 52) //top cover sides
			(41 40 52 53)
			(42 41 53 54)
			(43 42 54 55)
		);
	}

);

